  "icon": "data:image/svg+xml;utf8;base64,",
  "children": [
    {
      "en": {
        "title": "Connect DB",
        "description": "Connect to db from access file"
      },
      "es": {
        "title": "Conectarse a Base de Datos",
        "description": "Comando para conectarse a una base de datos desde un archivo access"
      },
      "form": {
        "css": "modal-lg",
        "inputs": [
          {
            "type": "file_select",
            "placeholder": {
              "es": "C:\\Ruta\\a\\basededatos.accdb",
              "en": "C:\\path\\to\\mydb.accdb"
            },
            "title": {
              "es": "Seleccione base de datos",
              "en": "Select DB:"
            },
            "description" : {
                "es" : "Conecta a la base de datos creada anteriormente. Formatos .mdb y .accdb son aceptados.",
                "en" : "Connects to a data base already created. It can handle .mdb and .accdb formats."
            },
            "id": "path",
            "css": "col-md-12"
          }
        ]
      },
      "video_youtube": "",
      "icon": "data:image/svg+xml;utf8;base64,",
      "module": "connect",
      "module_name": "Access",
      "visible": true,
      "options": false,
      "father": "module",
      "group": "scripts",
      "linux": false,
      "windows": true,
      "mac": true,
      "docker": true
    },
    {
      "en": {
        "title": "Execute query",
        "description": "Execute query to Access db"
      },
      "es": {
        "title": "Ejecutar query",
        "description": "Ejecuta query en una base de datos"
      },
      "form": {
        "css": "modal-lg",
        "inputs": [
          {
            "type": "input",
            "placeholder": {
              "es": "select * from tabla",
              "en": "select * from table"
            },
            "title": {
              "es": "Query",
              "en": "Query:"
            },
            "description":{
                "es" : "Query que se desea ejecutar en la base de datos.",
                "en" : "Query to execute in the connected database."
            },
            "id": "query",
            "css": "col-md-12"
          },
          {
            "type": "input",
            "placeholder": {
              "es": "Variable",
              "en": "Variable"
            },
            "title": {
              "es": "Asignar resultado a variable",
              "en": "Assign result to variable: "
            },
            "help": {
              "es": "Nombre de variable sin llaves {}",
              "en": "Variable name without {}"
            },
            "description":{
                "es" : "Variable donde guardar el resultado.",
                "en" : "Variable to save in the result."
            },
            "id": "var_",
            "css": "col-md-12"
          }
        ]
      },
      "video_youtube": "",
      "icon": "data:image/svg+xml;utf8;base64,",
      "module": "execute_query",
      "module_name": "Access",
      "visible": true,
      "options": false,
      "father": "module",
      "group": "scripts",
      "linux": false,
      "windows": true,
      "mac": true,
      "docker": true
